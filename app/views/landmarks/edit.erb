<h1>Edit a Landmark Here</h1>

<form action="/landmarks/<%= @landmark.id %>" method="post">

  <label for="landmark_name">Name: </label>
  <input type="text" name="landmark[name]" value="<%= @landmark.name %>"
    id="name">

  <br>

  <label for="year_completed">Year Completed: </label>
  <input type="text" name="landmark[year_completed]"
    value="<%= @landmark.year_completed %>" id="year_completed">

  <br>

  <input type="submit" value="Edit Landmark">
</form>
